#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n,r,sum=0;
    cin >> n;
    int temp=n;
    while(n>0){
        r=n%10;
        sum=sum+(r*r*r);
        n=n/10;
        
    }
    if(temp==sum){
        cout <<"Armstrong Number";
    }
    else{
        cout <<"Not A Armstrong Number";
    }
    return 0;
}
